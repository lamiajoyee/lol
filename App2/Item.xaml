    </Page.Resources>
	<Page.Background>
		<StaticResource ResourceKey="beguniPanelBg"/>
	</Page.Background>

    <Page.DataContext>
    	<Binding Source="{StaticResource Locator}"/>
    </Page.DataContext>

	<Grid d:DataContext="{d:DesignData /SampleData/MainViewModelSampleData.xaml}">
		<VisualStateManager.VisualStateGroups>
			<VisualStateGroup x:Name="VisualStateGroup"/>
		</VisualStateManager.VisualStateGroups>
		<Grid.Background>
			<ImageBrush ImageSource="Assets/lolBg.png" Opacity="0.37" Stretch="UniformToFill"/>
		</Grid.Background>
		<Grid.RowDefinitions>
			<RowDefinition Height="85"/>
			<RowDefinition/>
		</Grid.RowDefinitions>

		<Grid.DataContext>
			<ViewModel:MainViewModel/>
		</Grid.DataContext>

		<StackPanel Background="{StaticResource bgTopmostPanel}" Orientation="Horizontal" ScrollViewer.HorizontalScrollBarVisibility="Visible">
			<Image x:Name="image" Source="lol-logo-league-of-legends.png" VerticalAlignment="Bottom" Canvas.ZIndex="24" Margin="0,0,0,0" HorizontalAlignment="Left">
				<Interactivity:Interaction.Behaviors>
					<Core:EventTriggerBehavior EventName="PointerPressed">
						<Core:NavigateToPageAction TargetPage="App2.FirstPage"/>
					</Core:EventTriggerBehavior>
				</Interactivity:Interaction.Behaviors>
			</Image>
			<Button Style="{StaticResource ButtonStyle8}" HorizontalAlignment="Center" VerticalAlignment="Center" BorderThickness="0" ContentTemplate="{StaticResource DataTemplate6}" Padding="0" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" MinWidth="0" Width="50" ClickMode="Press" Margin="0">
				<Interactivity:Interaction.Behaviors>
					<Core:EventTriggerBehavior EventName="Tapped">
						<Core:NavigateToPageAction TargetPage="App2.MainPage"/>
					</Core:EventTriggerBehavior>
				</Interactivity:Interaction.Behaviors>
			</Button>
			<StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Width="200" Margin="6,0,0,0">
				<TextBlock HorizontalAlignment="Left" VerticalAlignment="Center" Foreground="{StaticResource champs}" FontSize="24" Text="Ancient Tome"/>
				<StackPanel  Orientation="Horizontal">
					<TextBlock Text="Cost:  " HorizontalAlignment="Left" VerticalAlignment="Center" Foreground="{StaticResource champs}" FontSize="14.667" FontStyle="Italic"/>
					<TextBlock Text="350g" FontSize="18.667" Margin="6,0,0,0"/>
				</StackPanel>
			</StackPanel>
		</StackPanel>
		<ScrollViewer Grid.Row="1" VerticalContentAlignment="Stretch">
			<Grid x:Name="grid" Margin="12">
				<Grid.RowDefinitions>
					<RowDefinition Height="Auto"/>
					<RowDefinition Height="Auto"/>
					<RowDefinition Height="Auto"/>
					<RowDefinition Height="Auto"/>
				</Grid.RowDefinitions>
				<Grid Margin="0,0,0,12" Background="{StaticResource beguniPanelBg}">
					<Grid.ColumnDefinitions>
						<ColumnDefinition Width="112"/>
						<ColumnDefinition Width="32*"/>
					</Grid.ColumnDefinitions>
					<Image Source="{Binding AllItems[0].itemImg}" Margin="12,0,0,0" HorizontalAlignment="Center" VerticalAlignment="Center" />
					<Grid Grid.Column="1">
						<Grid.RowDefinitions>
							<RowDefinition Height="Auto" />
							<RowDefinition Height="Auto" />
							<RowDefinition Height="Auto" />
						</Grid.RowDefinitions>
						<Grid Margin="12,12,12,0">
							<Grid.RowDefinitions>
								<RowDefinition Height="Auto" />
								<RowDefinition Height="Auto" />
							</Grid.RowDefinitions>
							<TextBlock Text="Static Bonuses" Foreground="{StaticResource champs}" FontSize="21.333"/>
							<TextBlock Text="+10 ability power +20 mana regeneration +2 golds" FontSize="14.667" Grid.Row="1" TextWrapping="Wrap" />
						</Grid>
						<Grid Grid.Row="1" Margin="12,12,12,0">
							<Grid.RowDefinitions>
								<RowDefinition />
								<RowDefinition />
							</Grid.RowDefinitions>
							<TextBlock Text="Passive" Foreground="{StaticResource champs}" FontSize="21.333"/>
							<TextBlock Text="Unique: Tribute (Limited to 1 Gold income item)" FontSize="14.667" Grid.Row="1" TextWrapping="Wrap" />
						</Grid>
						<Grid Grid.Row="2" Margin="12">
							<Grid.RowDefinitions>
								<RowDefinition />
								<RowDefinition />
							</Grid.RowDefinitions>
							<TextBlock Text="Active" Foreground="{StaticResource champs}" FontSize="21.333"/>
							<TextBlock Text="Unique: Battle Cry" FontSize="14.667" Grid.Row="1" TextWrapping="Wrap" />
						</Grid>
					</Grid>
				</Grid>
				<Grid Grid.Row="1" Margin="0,0,0,12" Background="{StaticResource beguniPanelBg}">
					<Grid.RowDefinitions>
						<RowDefinition Height="36"/>
						<RowDefinition Height="Auto"/>
					</Grid.RowDefinitions>
					<TextBlock Text="Builds Into" Foreground="{StaticResource champs}" Margin="12,12,0,0" FontSize="21.333"/>
					<GridView Grid.Row="1" ItemsSource="{Binding AllBuildsInto}" ItemTemplate="{StaticResource DataTemplate12}" Margin="12" Width="{Binding Width, ElementName=grid}"/>
				</Grid>
				<Grid Grid.Row="2" Margin="0,0,0,12" Background="{StaticResource beguniPanelBg}" >
					<Grid.RowDefinitions>
						<RowDefinition Height="36"/>
						<RowDefinition Height="Auto"/>
					</Grid.RowDefinitions>
					<TextBlock Text="Recipe" Foreground="{StaticResource champs}" Margin="12,12,0,0" FontSize="21.333"/>
					<StackPanel Grid.Row="1" Orientation="Horizontal" VerticalAlignment="Center">
						<GridView ItemsSource="{Binding AllRecipes}" ItemTemplate="{StaticResource DataTemplate14}" Height="65" VerticalContentAlignment="Stretch" Margin="12" VerticalAlignment="Center" />
						<TextBlock Text="30g" FontSize="21.333" VerticalAlignment="Center"/>
					</StackPanel>
				</Grid>
				<Grid Grid.Row="3" Margin="0" Background="{StaticResource beguniPanelBg}" >
					<Grid.RowDefinitions>
						<RowDefinition Height="36"/>
						<RowDefinition Height="Auto"/>
					</Grid.RowDefinitions>
					<TextBlock Text="Enchantments" Foreground="{StaticResource champs}" Margin="12,12,0,0" FontSize="21.333"/>
					<GridView Grid.Row="1" ItemsSource="{Binding AllEnchantments}" ItemTemplate="{StaticResource DataTemplate12}" Margin="12" Width="{Binding Width, ElementName=grid}"/>
				</Grid>
			</Grid>
			
		</ScrollViewer>
	</Grid>
</Page>
